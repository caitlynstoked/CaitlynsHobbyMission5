@{
    ViewData["Title"] = "Pricing";
}
<h1>@ViewData["Title"]</h1>

    <h1>Hourly Rate Calculator</h1>
    <p>Would you like to get into climbing? I can help you learn how to climb in a safe way that will allow you to become the mountain goat you've always wished you could be</p>

<div id="calculator">
    <label for="hours">Enter number of hours:</label>
    <input type="number" id="hours" min="0" step="0.5">
    <label>Hourly rate:</label>
    <input type="text" id="hourlyRate" readonly>
    <button id="calculateBtn">Calculate Total</button>
    <div id="totalOutput"></div>
    <button id="backToHome">Back to Home</button>

    
</div>

    
